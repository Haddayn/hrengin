#include <aw/platform/demangle.h>
#include <aw/types/string_view.h>

/* WARNING! Very long string(s). */

#include <aw/utility/ranges/paired.h>
#include <aw/utility/test.h>

const char mangled_verylong[] = "_";
const char demangled_verylong[] = "()";


const char* mangled[] = {
	"_ZTIN2aw4test11test_failedE",
	"_ZNSt7__cxx1112basic_stringIcSt11char_traitsIcESaIcEE6appendEPKc",
	"_ZN2aw6format12pretty_print5valueIRKNSt7__cxx1112basic_stringIcSt11char_traitsIcESaIcEEEEERS1_OT_"
};

const char* demangled[] = {
	"typeinfo for aw::test::test_failed",
	"std::__cxx11::basic_string<char, std::char_traits<char>, std::allocator<char> >::append(char const*)",
	"aw::format::pretty_print& aw::format::pretty_print::value<std::__cxx11::basic_string<char, std::char_traits<char>, std::allocator<char> > const&>(std::__cxx11::basic_string<char, std::char_traits<char>, std::allocator<char> > const&)"
};

TestFile("platform::demangle");

namespace aw {
Test( demangle )
{
	for (auto&& [man, dem] : paired(mangled, demangled))
		TestEqual( demangle( man ), dem );
	TestEqual( demangle( mangled_verylong ), demangled_verylong );
}

} // namespace aw

RunTests();
